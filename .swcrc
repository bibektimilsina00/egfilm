{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "dynamicImport": true
        },
        "transform": {
            "react": {
                "runtime": "automatic",
                "refresh": true
            }
        },
        "target": "es2017",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": false
    },
    "minify": false
}